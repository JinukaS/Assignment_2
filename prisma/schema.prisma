// file: prisma/schema.prisma

datasource db {
  provider = "mysql" // Should be mysql
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SavedOutput {
  id          String   @id @default(cuid())
  htmlContent String   @db.Text
  createdAt   DateTime @default(now())
}
